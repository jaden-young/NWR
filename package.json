{
  "name": "nwr_ts",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/install.js",
    "launch": "node scripts/launch.js",
    "build": "run-p build:*",
    "build:panorama": "tsc --project src/panorama/tsconfig.json",
    "build:vscripts": "tstl --project src/vscripts/tsconfig.json",
    "dev": "run-p dev:*",
    "dev:panorama": "tsc --project src/panorama/tsconfig.json --watch",
    "dev:vscripts": "tstl --project src/vscripts/tsconfig.json --watch",
    "dev:localizationCompiler": "tsc --project node_modules/~resource/tsconfig.json",
    "dev:localizationCompilerWatcher": "tsc --project node_modules/~resource/tsconfig.json --watch",
    "dev:runlocalizationWatcher": "node node_modules/@shushishtok/tooltip_generator/fsWatcher.js",
    "init": "link-module-alias && node node_modules/@shushishtok/tooltip_generator/init.js",
    "lint": "npx eslint src/ --ext .ts"
  },
  "devDependencies": {
    "@moddota/dota-lua-types": "^4.13.0",
    "@moddota/find-steam-app": "^1.1.0",
    "@moddota/panorama-types": "^1.12.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "npx": "^10.2.2",
    "prettier": "^2.4.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "typescript-to-lua": "^0.39.3"
  },
  "_moduleAliases": {
    "~resource": "game/resource",
    "~generator": "node_modules/@shushishtok/tooltip_generator"
  },
  "dependencies": {
    "@shushishtok/tooltip_generator": "^1.7.0",
    "link-module-alias": "^1.2.0",
    "node-watch": "^0.7.1",
    "valve-kv": "^1.1.0"
  },
  "description": "Dota 2 Custom",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalG/NWR.git"
  },
  "bugs": {
    "url": "https://github.com/DigitalG/NWR/issues"
  },
  "homepage": "https://github.com/DigitalG/NWR#readme"
}
